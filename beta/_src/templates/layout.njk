{% set meta = {
        url: 'http://nvenk.com',
        site: 'Nikhil Venkatesh - Product Designer'
}%}

<!DOCTYPE html>
<html lang="en">
{# <html manifest="nvenk.appcache"> #}
    <head>
        {% include "partials/meta.njk" %}

        <title>{{ meta2.title }}</title>
        <script src="js/vendor/scroll-restoration-polyfill.browser.js"></script>
        <script>
            history.scrollRestoration = 'manual';
        </script>
        <link rel="stylesheet" type="text/css" href="{{ var.styles }}" />
    </head>

    <body class="{{ var.bodyClass }}">

            {% block content %}{% endblock %}

            {% block scripts %}{% endblock %}

        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>

        <script src="{{ var.scripts }}"></script>
    </body>
</html>
